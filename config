HTTP_HELLO_DEPS="$ngx_addon_dir/src/ngx_http_hello.h               \
                "

HTTP_HELLO_SRCS="$ngx_addon_dir/src/ngx_http_hello.c               \
                "

ngx_module_type=HTTP
ngx_addon_name=ngx_http_hello_module
HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
ADDON_DEPS="$ADDON_DEPS $HTTP_HELLO_DEPS"
NGX_ADDON_SRCS="$NGX_ADDON_SRCS $NGX_ADDON_SRCS $HTTP_HELLO_SRCS"
